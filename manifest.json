{
  "manifest_version": 3,
  "name": "CustomGPT",
  "version": "1.0",
  "description": "Change your GPT's Background!",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://chat.openai.com/*"],
      "js": ["content_script.js"]
    }
  ]
}